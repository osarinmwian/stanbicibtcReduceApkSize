{
  "name": "sibtc",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "androidrelease": "react-native run-android --variant=release",
    "doc": "jsdoc -c jsdoc.json && git add .",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "assemble": "cd android && ./gradlew assembleRelease",
    "bundle": "cd android && ./gradlew bundleRelease",
    "preassemble": "cd android && ./gradlew clean",
    "prebundle": "cd android && ./gradlew clean",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "postinstall": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.1.5",
    "@hirbod/react-native-material-textfield": "^1.0.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "@reduxjs/toolkit": "^1.8.0",
    "@shopify/restyle": "^2.0.0",
    "expo": "^44.0.0",
    "expo-linear-gradient": "~11.0.3",
    "expo-splash-screen": "~0.14.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "^0.11.6",
    "formik": "^2.2.9",
    "i18next": "^21.6.14",
    "moti": "^0.16.1",
    "numeral": "^2.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.16.2",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~2.3.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-restart": "0.0.24",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-storyshots": "^5.3.21",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/numeral": "^2.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-docs": "^2.3.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "jest": "^25.5.4",
    "jest-expo": "^44.0.0",
    "jsdoc": "^3.6.7",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "~4.3.5"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{json,md,yaml,yml,js,jsx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  }
}
