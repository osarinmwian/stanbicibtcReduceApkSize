{
  "name": "sibtc",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "expo run:android",
    "androidrelease": "react-native run-android --variant=release",
    "doc": "jsdoc -c jsdoc.json && git add .",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "assemble": "cd android && ./gradlew assembleRelease",
    "bundle": "cd android && ./gradlew bundleRelease",
    "preassemble": "cd android && ./gradlew clean",
    "prebundle": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "expo": "^44.0.0",
    "expo-splash-screen": "~0.14.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.2",
    "moti": "^0.16.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-docs": "^2.3.2",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "jest-expo": "^44.0.0",
    "jsdoc": "^3.6.7",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.{json,md,yaml,yml,js,jsx}": [
      "prettier --write"
    ]
  }
}
